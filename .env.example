# Google Sheets Configuration
# Получите credentials из Google Cloud Console
GOOGLE_SHEETS_CREDENTIALS={"type":"service_account","project_id":"your-project-id",...}

# ID таблицы Google Sheets для торговых сигналов
GOOGLE_SHEETS_TRADING_SPREADSHEET_ID=your_spreadsheet_id_here

# Включение/выключение интеграции с Google Sheets
GOOGLE_SHEETS_ENABLED=true

# Trading Configuration
TRADING_ENABLED=true
TRADING_TAKE_PROFIT_PERCENT=2.0
TRADING_STOP_LOSS_PERCENT=2.0
TRADING_MAX_POSITIONS_PER_SYMBOL=1
TRADING_MAX_TOTAL_POSITIONS=10

# WebSocket Configuration
WS_PORT=3001

# API Configuration
API_PORT=3000
